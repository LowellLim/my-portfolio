		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"sass": "^1.57.1",
		"svelte": "^4.2.19",
		"svelte-check": "^4.1.4",
		"tslib": "^2.4.1",
		"typescript": "^5.7.3",
